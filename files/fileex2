import java.io.FileInputStream;

public class FileEx2 {
    public static void main(String[] a){
        try {
            FileInputStream fis = new FileInputStream("mydata.txt");
            int ch;

            while((ch = fis.read()) != -1) {
                System.out.print((char)ch);
            }
            fis.close();
        }
        catch(Exception e){
            System.out.println("Error: " + e.getMessage());
        }
    }
}
